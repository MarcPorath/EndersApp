<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:smartTable="sap.ui.comp.smarttable"
	xmlns:html="http://www.w3.org/1999/xhtml" controllerName="EndersApp.controller.searchDebDialog">
	<Dialog id="searchDebDialog" horizontalScrolling="true" showHeader="true" state="None" stretch="false" title="Title" type="Standard"
		verticalScrolling="true">
		<content>
			<IconTabBar expandable="false" expanded="true" applyContentPadding="false" backgroundDesign="Solid" upperCase="false" visible="true"
				selectedKey="Ok">
				<items>
					<IconTabFilter icon="sap-icon://collaborate" iconColor="Neutral" text="{i18n>bplan}" count="" design="Vertical" showAll="false"
						textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
						<content>
							<VBox id="vBox1">
								<items>
									<smartTable:SmartTable id="smartTable_ResponsiveTable" tableType="Table" editable="false" entitySet="Customer" useVariantManagement="true"
										showVariantManagement="true" useTablePersonalisation="true" header="Customer" showRowCount="true" useExportToExcel="false"
										enableAutoBinding="true">
										<smartTable:layoutData>
											<FlexItemData growFactor="1"/>
										</smartTable:layoutData>
										<Table mode="SingleSelectMaster" selectionChange="onItemPress"></Table>
									</smartTable:SmartTable>
								</items>
							</VBox>
						</content>
					</IconTabFilter>
					<IconTabSeparator icon="" iconDensityAware="false"/>
					<IconTabFilter icon="sap-icon://search" iconColor="Neutral" text="{i18n>dbsearch}" design="Vertical" showAll="false" textDirection="Inherit"
						enabled="true" visible="true" iconDensityAware="false">
						<content>
							<SearchField id="kunnr" placeholder="{i18n>kunnr}" visible="true" width="30%" liveChange="filterData" showSearchButton="false"/>
							<SearchField id="name1" placeholder="{i18n>name1}" visible="true" width="30%" liveChange="filterData" showSearchButton="false"/>
							<SearchField id="name2" placeholder="{i18n>name2}" visible="true" width="30%" liveChange="filterData" showSearchButton="false"/>
							<SearchField id="stras" placeholder="{i18n>stras}" visible="true" width="30%" liveChange="filterData" showSearchButton="false"/>
							<SearchField id="ort01" placeholder="{i18n>ort01}" visible="true" width="30%" liveChange="filterData" showSearchButton="false"/>
							<SearchField id="tel01" placeholder="{i18n>tel01}" visible="true" width="30%" liveChange="filterData" showSearchButton="false"/>
							<Button text="{i18n>searchButton}" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" press="setEntityST" iconDensityAware="false"/>
							<VBox id="vBox2">
								<items>
									<smartTable:SmartTable id="smartTable_searchResults" tableType="Table" editable="false" useVariantManagement="true"
										showVariantManagement="true" useTablePersonalisation="true" header="Customer" showRowCount="true" useExportToExcel="false"
										enableAutoBinding="true">
										<smartTable:layoutData>
											<FlexItemData growFactor="1"/>
										</smartTable:layoutData>
										<Table mode="SingleSelectMaster" selectionChange="onItemPress" id="table_deb"></Table>
									</smartTable:SmartTable>
								</items>
							</VBox>
						</content>
					</IconTabFilter>
				</items>
				<content>
					<Text text="Tab Bar Content goes here ..." width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
				</content>
			</IconTabBar>
		</content>
		<beginButton/>
		<buttons>
			<Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false"
				press="closeSearchDialog"/>
		</buttons>
		<customHeader/>
		<endButton/>
		<subHeader/>
	</Dialog>
</core:FragmentDefinition>